---
title: Galeria
script: gallery
---

<h1 class="section-heading">Galeria</h1>

<section class="content-area">
    <ul id="gallery" class="gallery reset-list">
        {{#each galeria}}
        <li class="item">
            <a href="/img/evento/{{ this }}" class="link">
                <img src="//imagizer.imageshack.us/150x100f/www.ayoshiirunning.com.br/img/evento/thumbs/{{ this }}" width="150" height="100" class="thumb">
            </a>
        </li>
        {{/each}}
    </ul>
</section>
